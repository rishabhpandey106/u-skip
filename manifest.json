{
    "manifest_version": 3,
    "name": "U-skip",
    "version": "1.0",
    "description": "Skips YouTube Ads",
    "host_permissions": ["*://www.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ]
  }
  